<h3>Student Management System</h3>
<p id="add_link"><a href="/adduser" class="add_student">Add Students</a></p>
<table>
    <thead>
        <tr>
            <th>S.No</th>
            <th>Name</th>
            <th>Age</th>
            <th>City</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        {{#each rows}}
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.name}}</td>
            <td>{{this.age}}</td>
            <td>{{this.city}}</td>
            <td> <a href="/edituser/{{this.id}}" class="edit">Edit</a></td>
            <td> <a onclick="return confirm('Are you sure?')" href="/deleteuser/{{this.id}}" class="delete">Delete</a></td>
        </tr>
        {{/each}}
    </tbody>
</table>