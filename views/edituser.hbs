<h3>Edit New Student</h3>
{{#if msg}}
<div class="alert">{{msg}}</div>
{{/if}}
{{#each rows}}
<form action="/edituser/{{this.id}}" method="post">
    <div class="form-group">
        <label for="name">Name</label>
        <input type="text" name="name" id="name" value="{{this.name}}">
    </div>
    <div class="form-group">
        <label for="age">Age</label>
        <input type="text" name="age" id="age" value="{{this.age}}">
    </div>
    <div class="form-group">
        <label for="city">City</label>
        <input type="text" name="city" id="city" value="{{this.city}}">
    </div>
    <div class="form-group">
        <button type="submit">Update</button>
        <a href="/" class="back">Back</a></td>
    </div>
</form>
{{/each}}